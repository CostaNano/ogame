    function handleCheckboxClick(data, id) {
        if (id == 'online') {
            $.ajax({
                url: "http:\/\/s117-en.ogame.gameforge.com\/game\/index.php?page=chatChangeSettings",
                type: 'POST',
                dataType: 'text',
                data: {
                    filter: 'chat_filter_online',
                    value: data.checked
                },
                success: function(data) {
                },
                error: function(jqXHR, textStatus, errorThrown) {
                }
            });
        } else if (id == 'active') {
            $.ajax({
                url: "http:\/\/s117-en.ogame.gameforge.com\/game\/index.php?page=chatChangeSettings",
                type: 'POST',
                dataType: 'text',
                data: {
                    filter: 'chat_filter_active',
                    value: data.checked
                },
                success: function(data) {
                },
                error: function(jqXHR, textStatus, errorThrown) {
                }
            });
        }
        return true;
    }
</script>
